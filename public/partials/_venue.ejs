<div class="container">
    <div class="row">

        <div class="col-xs-12 col-sm-4">
            <h3 class="section-title">
                <%- title %>
            </h3>
            <% if (googlemaps !== "") {%>
                <address>
                    <p>

                        <i class="fa fa-map-marker"
                           aria-hidden="true"></i>
                        <%- address %>
                    </p>
                    <p>

                        <i class="fa fa-globe"
                           aria-hidden="true"></i>
                        <a href="<%- web %>"
                           target="_blank">Sitio Web</a>
                    </p>
                    <p>
                        <i class="fa fa-phone"
                           aria-hidden="true"></i>
                        <%- phone %>

                    </p>
                </address>
        </div>

        <div class="col-sm-8 col-xs-12">
            <iframe src="<%- googlemaps %>"
                    width="100%"
                    height="450"
                    frameborder="0"
                    style="border:0"
                    allowfullscreen></iframe>
        </div>
        <% } else { %>
            <address>
                <p>
                    Sitio por concretar
                </p>
            </address>
    </div>
    <% } %>
</div>
</div>